</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" inline-output="false" labelreference="L95" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion2" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">AliceInit:=proc()# first step taken by Alice, setting the variables up
	local p,pm1,g,pm1_factorization;
	#p:=nextprime(rand());
	p:=nextprime(100);
	pm1:=p-1;
	pm1_factorization:=ifactor(pm1);
	g:=10; #g is the generator # I wasn't able to get this implementation right, because of time constraints
	return p,g,pm1_factorization;
	end proc;

AliceR:=proc(p)# function to generate r
	local pm1,r_func,r;
	pm1:=(p-1);
	r_func:=rand(1..pm1);
	r:=r_func();
	return r;
	end proc;

AliceMid:=proc(p,g,pm1_factorization,alice_r)#alice flipping the coin
	local n,r,a,b,pm1;
	pm1:=p-1;
	r:=alice_r;
	a:=(g^r) mod p;
	b:=(g^(r+((p-1)/2))) mod p;
	if coinflip() = 0 then return a,b;
	else return b,a;
	end if;
	end proc;

Bob:=proc(p,g,pm1_factorization,a,b)
	#if isGenerator(g, pm1_factorization) then print(&quot;ok&quot;);
	#end if;
	#if isCongruent(a^2,b^2,p) then print(&quot;ok&quot;);
	#end if;
	if coinflip() = 0 then return a;
	else return b;
	end if;
	end proc;

doit:=proc()
	local p,g,pm1_factorization,alice_r,a,b,bob_response;
	p,g,pm1_factorization:=AliceInit(); # to be used by Bob and Alice
	#print(p);
	alice_r:=AliceR(p);
	a,b:=AliceMid(p,g,pm1_factorization,alice_r);
	bob_response:=Bob(p,g,pm1_factorization,a,b);
	if bob_response = a then print(&quot;Bob Wins&quot;);
	else print(&quot;Alice Wins&quot;);
	end if;
	end proc;

for i in seq(1..10) do
	doit()
	end do;</EC-CodeEditor>
