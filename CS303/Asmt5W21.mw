<Group view="presentation" inline-output="false" labelreference="L146" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field style="Text" layout="Normal">For the Hash function, I didn't really pick any complex algoriothm, because I didn't want to cause more complications in the already computationally heavy algorithm</Text-field>
</Input>
</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" inline-output="false" labelreference="L102" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion0" expanded="true" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true"># Setup
H:=proc(m) # hash function
	return m;
	end proc;

generateL:=proc() # choosing a key length L
	local rng, n, l;
	rng:=rand(8..16);
	n:=rng();
	l:=(64*n);
	return l;
	end proc;

generateN:=proc() # choosing the modulus length N; such that N &lt; L and N &lt;= |H|
	local L,r;
	L:=[[1024, 160], [2048, 224], [2048, 256], [3072, 256]];
	r:=rand(1..4);
	return op(L[r()])
	end proc;

generateQ:=proc(N) # choosing an N-bit prime q
	return nextprime(2^N);
	end proc;

generateP:=proc(L,q) # choosing an L-bit prime p; such that p-1 is a multiple of q
	local p;
	p:=nextprime(2^L);
	while ( (p - 1) mod q ) &lt;&gt; 0 do
		p:=nextprime(p);
		end do;
	return p;
	end proc;

generateH:=proc(p) # choosing an integer h randomly from {2..p-2}
	local rng, h;
	rng:=rand(2..(p-2));
	h:=rng();
	return h;
	end proc;

computeG:=proc(h,p,q) # modular exponentiation
	return (h^((p-1)/q)) mod p; # g
	end proc;

generatePrivateKey:=proc(q) # generating Private key; choosing random integer from {1..q-1}
	local rng, x;
	rng:=rand(1..(q-1));
	x:=rng();
	return x;
	end proc;

generatePublicKey:=proc(g,x,p) # generating Public Key
	return (g^x) mod p;
	end proc;

generateK:=proc(q) # generating k; random integer from {1..q-1} ; to be used in signature
	local rng, k;
	rng:=rand(1..q-1);
	k:=rng();
	return k;
	end proc;

computeR:=proc(g,k,p,q) # computing r; to be used in signature
	return (floor((g^k) mod p)) mod q;
	end proc;

computeS:=proc(k,m,r,q,x) # computing s; to be used in signature
	local k_inv;
	k_inv:=1/k; # need to check
	return (floor( k_inv * ( H(m) + ( x * r ) ) )) mod q;
	end proc;

signature:=proc(p,q,g,m,private_key) # signing the message m with private_key
	local k,r,s;
	k:=generateK(q);
	r:=computeR(g,k,p,q);
	s:=computeS(k,m,r,q,private_key);
	while (r=0 or s=0) do # if r or s is zero, generate a new k and try again
		k:=generateK(q);
		r:=computeR(g,k,p,q);
		s:=computeS(k,m,r,q,private_key);
		end do;
	return (r,s);
	end proc;

verifySignature:=proc(q,r,s,public_key,m) # verify the signature
	local s_inv,w,u1,u2,v;
	if r &lt;= 0 or r &gt;= q then return false; end if;
	if s &lt;=0 or s &gt;= q then return false; end if;
	s_inv:=1/s; # need to check
	w:=s_inv mod q;
	u1:=H(m) * w mod q;
	u2:=r * w mod q;
	v:=(g^u1) * (public_key^u2) mod q;
	if v=r then return true;
	else return false;
	end if;
	end proc;</EC-CodeEditor>
