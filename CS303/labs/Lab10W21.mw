</Input>
</Group></Presentation-Block><Presentation-Block><CodeEditor-ExecGroup view="presentation" hide-input="false" inline-output="false" labelreference="L110" drawlabel="true" applyint="true" applyrational="true" display="code"><EC-CodeEditor id="CodeEditRegion2" expanded="false" visible="true" pixel-width="500" pixel-height="200" code-language="text/maple" autofit="true" wrapping="true" show-border="true" code-line-numbers="true">restart ;
use StringTools, Bits, NumberTheory in
plaintext := &quot;Only 20 days until spring!&quot;;
asciiList := map(Ord,Explode(plaintext));
pLength := StringTools[Length](plaintext);
bitList :=map(GetBits,asciiList,0..-1,bits=8);
bitLength := nops(bitList);
evalb(bitLength = pLength*8); #Check that things are the right length
encodedPlaintext := Bits[Join](bitList);
p :=NextSafePrime(2^bitLength);
q := NULL;  #Fill in the blank here.
r := NumberTheory[PrimitiveRoot](p);
g := Power(r,XXX) mod YYY; #fill in the blank here.
printf(&quot;Public key (p,g,q) is: (%d, %d, %d)\134n&quot;, p, g, q);

#Alice (the receiver of the plaintext) computes and publishes her public key 
#1.  Choose a random number between 3 and q.
a := rand(3..p)();
#2.  Compute g^a=A;
A := Power(g,XXXX) mod YYY; #fill in the blank here.
#3. Publish A
printf(&quot;A is:  %d&quot;, A);

#Bob (the sender of the plaintext) encrypts his text using A

#4.  Choose another random number between 3 and q.

k := rand(3..p)();
#5 compute c1, c2;

c1 := Power(g,XXX) mod YYY; #fill in the blank here.
c2 := (encodedPlaintext*Power(A,k)) mod p;
printf(&quot;(c1, c2) = (%d, \134n %d ) \134n&quot;, c1, c2);


#Alice attempts to decrypt c1, c2 using her secret value of a.

d1 := XXX mod YYY; #fill in the blank here.
evalb(encodedPlaintext=d1);  #They should check out.

#We can recover the ascii by using the bit and Char functions
asciiChunks := Bits[Split](d1,8);  #Split big number into 8 bit chunks
evalb(nops(asciiChunks)= nops(asciiList));  #check that decrypted character length is same as original.
charsFromChunks := map(Char,asciiChunks);  #convert each 8 bit chunk back into a character.
decryptedText := cat(op(charsFromChunks));  #concantenate the characters together into a single string.
evalb(decryptedText=plaintext); #check if we've made a round trip properly, should be true if they match.


end use;
</EC-CodeEditor>
